<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <!--Required metadata-->
    <id>$id$</id>
    <version>$version$</version>
    <description>$description$</description>
    <authors>$authors$</authors>

    <!--Optional meatadata-->
    <owners>$owners$</owners>
    <projectUrl>$projectUrl$</projectUrl>
    <license type="file">$licenseFileName$</license>
    <icon>images\$iconFileName$</icon>
    <requireLicenseAcceptance>$requireLicenseAcceptance$</requireLicenseAcceptance>
    <releaseNotes>$releaseNotes$</releaseNotes>
    <copyright>$copyright$</copyright>
    <tags>$packageTags$</tags>

    <dependencies>
      <group targetFramework=".NETStandard2.0">
        <dependency id="Azure.Core" version="$AzureCoreVersion$" exclude="Build,Analyzers" />
        <dependency id="Azure.Identity" version="$AzureIdentityVersion$" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Bcl.AsyncInterfaces" version="$MicrosoftBclAsyncInterfacesVersion$" exclude="Build,Analyzers" />

        <dependency id="CommandLineParser" version="$CommandLineParserVersion$" exclude="Build,Analyzers" />
        <dependency id="Microsoft.ApplicationInsights" version="$MicrosoftApplicationInsightsVersion$" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Diagnostics.NETCore.Client" version="$MicrosoftDiagnosticsNETCoreClientVersion$" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Extensions.Logging.Debug" version="$MicrosoftExtensionsLoggingDebugVersion$" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Extensions.Options" version="$MicrosoftExtensionsOptionsVersion$" exclude="Build,Analyzers" />
        <dependency id="System.Diagnostics.PerformanceCounter" version="$SystemDiagnosticsPerformanceCounterVersion$" exclude="Build,Analyzers" />
        <dependency id="System.IdentityModel.Tokens.Jwt" version="$SystemIdentityModelTokensJwtVersion$" exclude="Build,Analyzers" />
        <dependency id="System.Text.Json" version="$SystemTextJsonVersion$"  exclude="Build,Analyzers" />
        <dependency id="System.ValueTuple" version="$SystemValueTupleVersion$" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <!-- License and Icon -->
    <file src="..\ReleaseReousrces\$licenseFileName$" target="" />
    <file src="..\ReleaseReousrces\$iconFileName$" target="images\" />

    <!--Assemblies-->
    <file src="bin\$configuration$\netstandard2.0\Microsoft.ApplicationInsights.Profiler.Core.dll" target="lib\netstandard2.0"></file>
    <!--Dependencies-->
    <file src="bin\$configuration$\netstandard2.0\Microsoft.ApplicationInsights.Profiler.Logging.dll" target="lib\netstandard2.0"></file>
    <file src="bin\$configuration$\netstandard2.0\Microsoft.ServiceProfiler.Agent.Exceptions.dll" target="lib\netstandard2.0"></file>
    <file src="bin\$configuration$\netstandard2.0\Microsoft.ServiceProfiler.Agent.FrontendClient.dll" target="lib\netstandard2.0"></file>
    <file src="bin\$configuration$\netstandard2.0\Microsoft.ServiceProfiler.Agent.FrontendClient.Profiler.dll" target="lib\netstandard2.0"></file>
    <file src="bin\$configuration$\netstandard2.0\Microsoft.ServiceProfiler.Common.Utilities.dll" target="lib\netstandard2.0"></file>
    <file src="bin\$configuration$\netstandard2.0\Microsoft.ServiceProfiler.Contract.Agent.dll" target="lib\netstandard2.0"></file>
    <file src="bin\$configuration$\netstandard2.0\Microsoft.ServiceProfiler.Contract.Agent.Profiler.dll" target="lib\netstandard2.0"></file>
    <file src="bin\$configuration$\netstandard2.0\Microsoft.ServiceProfiler.Contract.Http.dll" target="lib\netstandard2.0"></file>
    <file src="bin\$configuration$\netstandard2.0\Microsoft.ServiceProfiler.HttpPipeline.dll" target="lib\netstandard2.0"></file>
    <file src="bin\$configuration$\netstandard2.0\Microsoft.ServiceProfiler.Orchestration.dll" target="lib\netstandard2.0"></file>
    <file src="bin\$configuration$\netstandard2.0\Microsoft.ServiceProfiler.Utilities.dll" target="lib\netstandard2.0"></file>
  </files>
</package>
